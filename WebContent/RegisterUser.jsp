<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Enroll in National Bank</title>
<link rel="stylesheet" href="styles.css">
<script type="text/javascript" src="script.js"></script>
</head>
<body>
	<div class="navbar">
		<table>
			<tr>
				<td style="padding-left: 150px;">National Bank Logo</td>
				<td style="padding-left: 150px;"><a href="RegisterUser.jsp">Sign
						Up</a></td>
				<td style="padding-left: 150px;"><a href="Login.jsp">Sign
						In</a></td>
		</table>
	</div>
	<br>
	<center>
		<h2>Let's Get Started</h2>
	</center>
	<br>
	<center>
		<p style="font-size: 15px; font-weight: bold">Thank you for
			choosing National Bank. Before enrollment, We need some information
			to verify your identity:</p>
	</center>
	<br>
	<br>
	<table>

		<tr>
			<td><img
				src="data:image/png;base64,"
				style="height: 150px; width: 150px; padding-left: 600px;"SSN"></img>
				<p style="padding-left: 600px; font-size: 15px;">Your Social
					Security Number</p></td>

			<td><img
				src="https://media.istockphoto.com/vectors/mobile-phone-icon-vector-line-art-outline-smartphone-symbol-simple-vector-id899623028?k=20&m=899623028&s=612x612&w=0&h=SpcOcFlrZnCmbBMjKTgP2zcclbkQOrMKxoSPKCd5sVo="
				style="height: 200px; width: 200px; padding-left: 150px;"PhoneNumber"></img>
				<p style="padding-left: 190px; font-size: 15px;">Your Phone
					Number</td>

			<td><img
				src="https://us.123rf.com/450wm/vasilyrosca/vasilyrosca1909/vasilyrosca190900051/130091502-driver-licence-icon-driver-id-card-vector-license-drive-identity-photo-identification.jpg?ver=6"
				style="height: 200px; width: 200px; padding-left: 225px;"Driver'sLicense"></img>
				<p style="padding-left: 255px; font-size: 15px;">Your Driver's
					License</td>
		</tr>

	</table>
	<br>
	<br>
	<br>
	
	<div style="color: #FF0000; font-size: 17px; padding-left: 800px;">${errorMessage}</div>
	<div style="color: #148a01; font-size: 17px; padding-left: 800px;">${confirmation}</div>

	<form onsubmit="return checkCredentials(document.getElementById('SSN').value, document.getElementById('DriverLicenseNum').value)"
		method="post" action="UserRegistration" name="Registration">
		<table>
			<tr>
				<td><h2
						style="font-size: 25px; font-family: Arial; padding-left: 600px;">
						Personal Information
						</h1></td>
			</tr>
			<br>
			<br>
			<tr>
				<td><div style="padding-left: 600px;">
						<label for="SSN">Social Security Number:</label><br>
						<input type="text" name="SSN" id="SSN" size="25"
							maxlength=30 required>
					</div></td>

				<td><div style="padding-left: 150px;">
						<label for="DriverLicenseNum">Driver's License Number:</label><br>
						<input type="text" name="DriverLicenseNum" id="DriverLicenseNum" size="25" maxlength=30 required>
					</div></td>

			</tr>

			<tr>
				<td><div style="padding-left: 600px;">
						<a id="showOther" href="#" onclick="ToggleOther()">Don't have
							a Social Security Number?</a>
					</div></td>
			</tr>

			<tr>

				<td><div style="padding-left: 600px;">
						<div id="PassportNum" style="display: none;">
							<label for="PassportNum">Passport Number:</label><br> <input
								type="text" name="PassportNum" size="25" maxlength=30>

						</div>
					</div></td>

			</tr>

			<tr>
				<td><div style="padding-left: 600px;">
						<div id="StateNum" style="display: none">
							<label for="StateNum">State-Issued ID Number:</label><br> <input
								type="text" name="stateNum" size="25" maxlength=30>
						</div>

					</div></td>

			</tr>

			<tr>
				<td><div style="padding-left: 600px;">
						<label for="FirstName">First Name:</label><br> <input
							type="text" name="FirstName" id = "FirstName" size="25" maxlength=30>
					</div></td>

				<td><div style="padding-left: 150px;">
						<label for="LastName">Last Name:</label><br> <input
							type="text" name="LastName" id = "LastName" size="25" maxlength=30>
					</div></td>

				<td><div style="padding-left: 150px;">
						<label for="birthDate">Date Of Birth:</label><br> <input
							type="date" name="birthDate" size="25" maxlength=30>
					</div></td>

			</tr>
			<tr>

				<td><div style="padding-left: 600px; padding-top: 50px;">
						<label for="email">Email:</label><br> <input type="text"
							name="email" size="25" maxlength=30>
					</div></td>

				<td><div style="padding-left: 150px; padding-top: 50px;">
						<label for="phone">Phone:</label><br> <input type="phone"
							name="phone" size="25" maxlength=10>
					</div></td>
			</tr>
			<tr>

				<td><div style="padding-left: 600px; padding-top: 50px;">
						<label for="username">Username:</label><br> <input
							type="text" name="username" size="25" maxlength=30>
					</div></td>

				<td><div style="padding-left: 150px; padding-top: 50px;">
						<label for="password">Password:</label><br> <input type="password"
							name="password" size="25" maxlength=10>
					</div></td>


			</tr>

		</table>

		<br> <br> <br>
		<center>
			<input
				style="font-size: 15px; font-weight: bold; height: 30px; width: 75px; background-color: #b21e04"
				type="submit" value="submit">
		</center>

	</form>
</body>
</html>